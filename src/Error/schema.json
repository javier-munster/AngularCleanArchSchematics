{
    "$schema": "http://json-schema.org/schema",
    "$id": "Error",
    "title": "Error Options",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the Error"
        },
        "feature": {
            "type": "string",
            "description": "The name of the feature this Error belongs to"
        }
    },
    "required": ["name", "feature"]
}

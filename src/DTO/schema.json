{
    "$schema": "http://json-schema.org/schema",
    "$id": "DTO",
    "title": "Data Transfer Object Options",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the DTO"
        },
        "feature": {
            "type": "string",
            "description": "The name of the feature this DTO belongs to"
        },
        "isAsync": {
            "type": "boolean",
            "description": "If the return type is async",
            "default": true
        }
    },
    "required": ["name", "feature"]
}
